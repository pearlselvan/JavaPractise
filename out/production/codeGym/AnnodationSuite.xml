<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="7" verbose="1" name="WebTestThreaded Suite" skipfailedinvocationcounts="false" junit="false"


    parallel="false" data-provider-thread-count="50" annotations="JDK">


    <listeners>
        <listener class-name="AnnodationDemo.TestListener" />
    </listeners>

    <!-- SELENIUM CONFIGURATION -->
    <parameter name="mobileNodeType" value="appium" />

    <parameter name="seleniumhost" value="127.0.0.1" />
    <parameter name="seleniumport" value="4723" />
    <test verbose="2" name="mobile-appium-android-test" annotations="JDK">
        <classes>
            <class name="AnnodationDemo.AndroidTest"></class>

        </classes>
    </test>

</suite>

